{{#define id="datosEmbarazoActualAvanzadosSis"}}
  {{#section id="base"}}
    {{field id="inicial" type="expr" value="=atencionEmbarazada=='primeraVez'"}}
    {{field id="subsecuente" type="expr" value="=atencionEmbarazada=='subsecuente'"}}
    {{field id="tieneAtencion" type="expr" value="=calc.in(atencionEmbarazada,['primeraVez','subsecuente'])"}}
    {{#field id="atencionEmbarazada"}}
      {{onChange clearFields="planSeguridad,planSeguridad1"}}
    {{/field}}
    {{validator type="expr" validIf="=(@numeroConsultasRecibidas<31)" message="Las consultas no deben ser entre 0 y 30."}}
    {{validator type="expr" notValidIf="=medicamentosDuranteEsteEmbarazo=='noAplica'" message="Prescripción Ácido Fólico / Otras Acciones Inválido"}}
    {{validator type="expr" notValidIf="=planSeguridad1=='noAplica'" message="Plan Seguridad Inválido"}}
    {{validator type="expr" notValidIf="=planSeguridad=='noAplica'" message="Plan Seguridad Inválido"}}
    {{validator type="expr" notValidIf="=complicacionTraslado=='noAplica'" message="Traslado Embarazada Inválido"}}
    {{validator type="expr" notValidIf="=accionesTraslado=='noAplica'" message="Otras Acciones Traslado AME Inválido"}}
    {{validator type="expr" notValidIf="=primeraVezAltoRiesgo=='noAplica'" message="Primera Vez Alto Riesgo Inválido"}}
  {{/section}}
{{/define}}