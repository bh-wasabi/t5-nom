{{#define id="datosGeneralesDetecciones"}}
  {{#section id="base"}}
    {{field id="esMayor60" type="expr" value="=calc.fromNowYears(persona.fechaNacimiento)>=60"}}
    {{field id="esMayor50" type="expr" value="=calc.fromNowYears(persona.fechaNacimiento)>=50"}}
    {{field id="esMayor20" type="expr" value="=calc.fromNowYears(persona.fechaNacimiento)>=20"}}
  {{/section}}
{{/define}}