{{#define id="datosServiciosSeulEgreso"}}
  {{#section id="base"}}
    {{field id="siServicioNormal" type="expr" value="tipoServicioOtorgado=='normal'"}}
    {{field id="siOtraProcedencia" type="expr" value="=procedenciaIngreso=='5'"}}
    {{field id="siReferido" type="expr" value="=procedenciaIngreso=='3'"}}
    {{!-- {{field id="siServicioAdicional" type="expr" value="=numeroServicio=='uno'||numeroServicio=='dos'"}} --}}
    {{field id="tieneTerapiaIntensiva" type="expr" value="=calc.in(@claveServicioAdicional,['unidadCoronaria','unidadTerapiaIntensivaQuirurgica','unidadTerapiaIntensiva','unidadTerapiaIntensivaQuirurgicaPediatrica','unidadTerapiaIntensivaPediatrica','unidadCuidadosIntensivosNeonatales'])"}}
    {{field id="tieneTerapiaIntermedia" type="expr" value="=calc.in(@claveServicioAdicional,['unidadTerapiaIntermedia','crecimientoDesarrollo','unidadCuidadosIntermediosNeonatales'])"}}
    {{#field id="tipoServicioOtorgado"}}
      {{onChange clearFields="servicioIngreso,numeroServicio,claveServicioAdicional,terapiaIntensiva,terapiaIntermedia"}}
    {{/field}}
    {{#field id="claveServicioAdicional"}}
      {{onChange clearFields="terapiaIntensiva,terapiaIntermedia"}}
    {{/field}}
    {{#field id="procedenciaIngreso"}}
      {{onChange clearFields="especifiqueProcedencia,cluesProcedencia"}}
    {{/field}}
    {{#onChange}}
  		{{#update}}
  			{{set numeroServicio="=claveServicioAdicional.length"}}
  		{{/update}}
  	{{/onChange}}
    {{validator type="expr" notValidIf="=procedenciaIngreso=='4'&&moment().diff(moment(fechaNacimiento), 'months')>1" message="Procedencia Ingreso Inválida"}}
    {{validator type="expr" notValidIf="=@cluesProcedencia==user.establishmentData.clues" message="CLUES Procedencia Inválido"}}
    {{validator type="expr" validIf="=!calc.in(calc.lowerCase(calc.normalize(@especifiqueProcedencia)),['consulta externa','urgencias','referido','cunero patologico','otro'])" message="Especifique Procedencia Inválida"}}
    {{validator type="expr" notValidIf="=@claveServicioAdicional.length>2" message="Máximo dos Servicios Adicionales"}}
    {{validator type="expr" notValidIf="=calc.in(@claveServicioEgreso,@claveServicioAdicional)" message="='Servicio ' + @_claveServicioEgreso + ' Duplicado'"}}
    
    {{validator type="expr" notValidIf="=moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>=18&&calc.in(@servicioIngreso,fn('servicioEspecialidadPediatria'))" message="Servicio Ingreso Inválido (Paciente Mayor de Edad)"}}
    {{validator type="expr" notValidIf="=moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>=18&&calc.in(@claveServicioEgreso,fn('servicioEspecialidadPediatria'))" message="Clave Servicio Egreso Inválido (Paciente Mayor de Edad)"}}
    {{validator type="expr" notValidIf="=moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>=18&&calc.arrayInArray(@claveServicioAdicional,fn('servicioEspecialidadPediatria'))" message="Clave Servicio Adicional Inválido (Paciente Mayor de Edad)"}}
    
    {{validator type="expr" notValidIf="=(_parentDoc.persona.genero!=='mujer'&&calc.in(@servicioIngreso,fn('servicioEspecialidadGinecoobstetriciaEgresos')))||(_parentDoc.persona.genero==='mujer'&&calc.in(@servicioIngreso,fn('servicioEspecialidadGinecoobstetriciaEgresos'))&&(moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')<9||moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>59))" message="Servicio Ingreso Inválido (Paciente No Mujer o Fuera de Rango de Edad)"}}
    {{validator type="expr" notValidIf="=(_parentDoc.persona.genero!=='mujer'&&calc.in(@claveServicioEgreso,fn('servicioEspecialidadGinecoobstetriciaEgresos')))||(_parentDoc.persona.genero==='mujer'&&calc.in(@claveServicioEgreso,fn('servicioEspecialidadGinecoobstetriciaEgresos'))&&(moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')<9||moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>59))" message="Clave Servicio Egreso Inválido (Paciente No Mujer o Fuera de Rango de Edad)"}}
    {{validator type="expr" notValidIf="=(_parentDoc.persona.genero!=='mujer'&&calc.arrayInArray(@claveServicioAdicional,fn('servicioEspecialidadGinecoobstetriciaEgresos')))||(_parentDoc.persona.genero==='mujer'&&calc.arrayInArray(@claveServicioAdicional,fn('servicioEspecialidadGinecoobstetriciaEgresos'))&&(moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')<9||moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>59))" message="Clave Servicio Adicional Inválido (Paciente No Mujer o Fuera de Rango de Edad)"}}
    
    {{validator type="expr" notValidIf="=_parentDoc.persona.genero!=='mujer'&&calc.in(@servicioIngreso,['ginecologia','urologiaGinecologica'])" message="Servicio Ingreso Inválido (Paciente No Mujer)"}}
    {{validator type="expr" notValidIf="=_parentDoc.persona.genero!=='mujer'&&calc.in(@claveServicioEgreso,['ginecologia','urologiaGinecologica'])" message="Clave Servicio Egreso Inválido (Paciente No Mujer)"}}
    {{validator type="expr" notValidIf="=_parentDoc.persona.genero!=='mujer'&&calc.arrayInArray(['ginecologia','urologiaGinecologica'],@claveServicioAdicional)" message="Clave Servicio Adicional Inválido (Paciente No Mujer)"}}
  {{/section}}
{{/define}}