{{#define id="datosCondicionPlanificacion"}}
  {{#section id="base"}}
    {{field id="telemedicina" type="expr" value="=@solicitaTelemedicina=='si'?'En Tiempo Real':''"}}
    {{field id="teleconsulta" type="expr" value="=(@solicitaTeleconsulta=='si'&&calc.isNotEmpty(estudiosTeleconsulta))?'Diferida':''"}}
    {{#field id="solicitaTelemedicina"}}
      {{#onChange}}
        {{!-- {{onchange clearFields="modalidad"}} --}}
        {{set modalidad="=@solicitaTelemedicina=='si'?'En Tiempo Real':''"}}
      {{/onChange}}
    {{/field}}
    {{#field id="estudiosTeleconsulta"}}
      {{#onChange}}
        {{!-- {{onchange clearFields="modalidad"}} --}}
        {{set modalidad="=(@solicitaTeleconsulta=='si'&&calc.isNotEmpty(estudiosTeleconsulta))?'Diferida':''"}}
      {{/onChange}}
    {{/field}}
    {{!-- {{validator type="expr" validIf="=1==90" message="Error en Preservativos Entregados" log="=calc.concat(telemedicina,teleconsulta)"}} --}}
  {{/section}}
{{/define}}