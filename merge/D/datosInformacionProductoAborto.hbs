{{#define id="datosInformacionProductoAborto"}}
  {{#section id="base"}}
    {{#field id="folioCertificadoMuerteFetal"}}
      {{onChange clearFields="folioCertificadoUtilizado"}}
    {{/field}}
    {{#onChange}}
      {{#update}}
        {{set folioCertificadoUtilizado="=@folioCertificadoMuerteFetal"}}
      {{/update}}
    {{/onChange}}
    {{validator type="expr" notValidIf="=@folioCertificadoMuerteFetal&&fn('validaCertificadoMuerteFetal',@folioCertificadoMuerteFetal,_mainDoc._type=='notaSeulEgreso'?calc.getRef(_mainDoc, 'base.fechaEgreso'):calc.getRef(_mainDoc, 'base.fechaAtencion'))" message="Certificado Muerte Fetal Inv√°lido"}}
  {{/section}}
{{/define}}