{{#define id="eventoLesionSeul"}}
  {{#section id="base"}}
    {{field id="fechaAtencion1" type="expr" value="=@fechaOcurreEvento"}}
    {{validator type="expr" validIf="=fechaNacimiento<fechaAtencion1" message="Error en la fecha atención"}}
    {{#field id="sepomex" type="text" label="SEPOMEX"}}
      {{#onChange}}
        {{set codigoPostal="=codigoPostal"}}
        {{set entidad="=colonia"}}
        {{set municipio="=municipio"}}
        {{set localidad="=localidad"}}
        {{set estado="=estado"}}
      {{/onChange}}
    {{/field}}
    {{#field id="desconoceAsentamiento"}}
      {{#onChange clearFields="nombreAsentamiento"}}
        {{set nombreAsentamiento="=desconoceAsentamiento=='sinInformacion'?'SIN INFORMACIÓN':desconoceAsentamiento=='noEspecificado'?'NO ESPECIFICADO':''"}}
      {{/onChange}}
    {{/field}}
  {{/section}}
{{/define}}