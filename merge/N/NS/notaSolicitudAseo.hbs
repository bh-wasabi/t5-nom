{{#define id="notaSolicitudAseo"}}
  {{#action id="afectar" error="=calc.getRef(base,'aseo.base.ubicacion')?null:'Solicitud Incorrecta'"}}
    {{#update section="base"}}
      {{setRef condition="=aseo.base.subTipoSolicitud=='reposicionInsumos'" ref="aseo.base.vencimiento" value="=calc.dueDate(aseo.base.vencimiento, 'atencion', calc.dueTimes('aseo', aseo.base.subTipoSolicitud, aseo.base.motivo, aseo.base.momentoDieta, aseo.base.criticidadZona, aseo.base.tiempos))"}}
      {{setRef condition="=aseo.base.subTipoSolicitud=='reposicionInsumos'" ref="aseo.base.alerta"      value="=calc.alertDate(aseo.base.vencimiento, 'atencion', calc.dueTimes('aseo', aseo.base.subTipoSolicitud, aseo.base.motivo, aseo.base.momentoDieta, aseo.base.criticidadZona, aseo.base.tiempos))"}}
    {{/update}}
  {{/action}}
  {{#grid id="adjuntos" section="adjuntos" allowRemove="true" allowSort="true" sortBy="orden, fecha"}}
    {{column field="nombre" width="350"}}
    {{column field="tipo" width="200"}}
    {{column field="fecha" width="100" readOnly="true"}}
    {{column field="ext" width="100" readOnly="true"}}
    {{column field="tamano" width="100" readOnly="true"}}
    {{column field="orden" width="60"}}
  {{/grid}}
{{/define}}