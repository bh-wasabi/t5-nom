{{#define id="notaSaludMental"}}
  {{#section id="base"}}
    {{field id="tienePreconsulta" type="expr" value="=catServicios=='preconsulta'"}}
    {{field id="fechaAtencion1" type="expr" value="@fechaAtencion"}}
    {{validator type="expr" validIf="=persona.fechaNacimiento<fechaAtencion1" log="=user.job" message="Error en la fecha atenciÃ³n"}}
  {{/section}}
  {{#section id="diagnosticos"}}
    {{field id="esPsicologo" type="expr" value="=calc.in(Utils.getUserProfile().service[0],['psicologiaAdultos','psicologiaPediatrica','psicologiaClinica'])"}}
    {{validator type="expr" notValidIf="=calc.findDuplicates(calc.pluckRef(consumoSustanciaPsicoactivas,'base.consumoSustanciasPsicoactivas'))" message="Datos Duplicados"}}
    {{!-- {{validator type="expr" notValidIf="=1==1" log="=calc.in(Utils.getUserProfile().service[0],['psicologiaAdultos','psicologiaPediatrica','psicologiaClinica'])" message="Es Psicologo"}} --}}
  {{/section}}
{{!--RACC VISTA  --}}
 {{#view id="ultima"}}
   {{#find limit="1"}}
     {{include field="_name"}}
     {{sort field="_id" direction="asc"}}
     {{filter field="base.estatus" eq="='afectado'"}}
     {{filter condition="=persona" field="persona.id" eq="=persona"}}
     {{!-- {{filter condition="=desde" field="_create.date" gt="=desde"}} --}}
   {{/find}}
 {{/view}}
{{/define}}