{{#define id="notaAltaServicio"}}
  {{#section id="base"}}
    {{field id="tieneTraslado" type="expr" value="=motivoAltaServicio=='trasladoOtroUnidad'"}}
    {{!-- RACC --}}
    {{field id="defuncion" type="expr" value="=motivoAltaServicio=='defuncion'"}}
    {{!-- {{field id="esMinisterio" type="expr" value="=ministerioPublico=='no'"}} --}}
    {{!-- RACC --}}
    {{#field id="central"}}
      {{onChange clearFields="cama"}}
    {{/field}}
    {{#field id="cama"}}
      {{#editor alwaysLoad="true"}}
        {{param estatus="='preAlta'"}}
        {{param central="=central"}}
      {{/editor}}
    {{/field}}
    {{#field id="interconsultas"}}
      {{#editor}}
        {{param persona="=persona.id"}}
      {{/editor}}
    {{/field}}
    {{validator type="expr" notValidIf="=@CLUES==user.job.cluesAdscripcionNomina" message="CLUES Inv치lido"}}
    {{validator type="expr" validIf="=moment(@fechaIngreso).isAfter(@fechaIngresoUrgencias)" message="Fecha Desde Inv치lida"}}
    {{validator type="expr" validIf="=moment(@fechaEgreso).isAfter(@fechaIngresoUrgencias)" message="Fecha Hasta Inv치lida"}}
    {{validator type="expr" validIf="=calc.isEmpty(@interconsultas)||@interconsultas.length<=3" message="M치ximo 3 Interconsultas"}}
  {{/section}}
  {{#grid id="solicitudesEgreso" section="solicitudesEgreso" allowInsert="false" allowRemove="false"}}
    {{column field="_name" width="800" readOnly="true"}}
    {{column field="_moment" width="250" readOnly="true"}}
    {{column field="accion" width="90"}}
  {{/grid}}
{{/define}}