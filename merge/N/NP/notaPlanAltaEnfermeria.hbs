{{#define id="notaPlanAltaEnfermeria"}}
  {{#section id="base"}}
    {{#field id="medicamentosMinistrados"}}
      {{!-- {{editor display="base.codigo"}} --}}
      {{editor display="base.subclave"}}
    {{/field}}
    {{field id="cantidadMedicamentosMinistrados" type="calc" value="=medicamentosMinistrados.length"}}
    {{validator type="expr" notValidIf="=medicamentosMinistrados.length>6" message="MÃ¡ximo 6 Medicamentos Ministrados"}}
  {{/section}}

  {{#view id="buscarPlanAltaEnfermeria"}}
    {{#find limit="-1"}}
      {{include field="base"}}
      {{sort field="_created.date" direction="desc"}}
      {{filter field="_created.request" eq="=request"}}
    {{/find}}
  {{/view}}
{{/define}}