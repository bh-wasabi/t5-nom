    {{#field id="medicamentosMinistrados"}}
      {{!-- {{editor display="base.codigo"}} --}}
      {{editor display="base.subclave"}}
    {{/field}}
  {{/section}}

  {{#view id="buscarPlanAltaEnfermeria"}}
