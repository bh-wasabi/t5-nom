    {{#field id="central"}}
      {{onChange clearFields="cama"}}
    {{/field}}
    {{#field id="cama"}}
      {{#editor alwaysLoad="true"}}
        {{param estatus="='ocupada'"}}
        {{param central="=central"}}
      {{/editor}}
    {{/field}}
  {{/section}}
{{/define}}