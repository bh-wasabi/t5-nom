{{#define id="notaDerechohabienciaEgreso"}}
  {{#section id="base"}}
    {{field id="tieneOtro" type="expr" value="=calc.in(dependencia,['otra'])&&user.establishmentData.cluesIdEntidad=='09'"}}
    {{field id="lista" type="expr" value="=dependenciaLista"}}
    {{#field id="dependencia"}}
      {{onChange clearFields="otra"}}
    {{/field}}
    {{validator type="expr" validIf="=calc.in(@dependencia, @lista)||calc.in(@dependencia,['noEspecificado','ninguna','seIgnora'])" message="Dependencia No Configurada en Paciente"}}
  {{/section}}
{{/define}}