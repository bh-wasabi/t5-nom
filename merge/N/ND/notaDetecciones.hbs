{{#define id="notaDetecciones"}}
  {{#section id="tamizaje"}}
    {{#field id="tamizajeTristeDeprimido"}}
      {{onChange clearFields="deteccionDepresion"}}
    {{/field}}
    {{#field id="olvidaCosasCostumbre"}}
      {{onChange clearFields="deteccionAlteracionMemorai"}}
    {{/field}}
    {{#field id="tamizajeFugaOringa"}}
      {{onChange clearFields="deteccionIncontinenciaUrinaria"}}
    {{/field}}
    {{#field id="tamizajeCaidas"}}
      {{onChange clearFields="caida60Mas"}}
    {{/field}}
    {{#field id="tamizajeActividadesBasicas"}}
      {{onChange clearFields="evaluacionActividadesBasicas"}}
    {{/field}}
    {{#field id="tamizajeActividadesInstrumental"}}
      {{onChange clearFields="evaluacionActividadesInstrumental"}}
    {{/field}}
  {{/section}}
  
  {{#section id="datosGenerales"}}
    {{#field id="edadCuidador"}}
      {{onChange clearFields="sexoCuidador,sobrecargaCuidado"}}
    {{/field}}
  {{/section}}

  {{#section id="otrosDatos"}}
    {{#field id="hiperplasiaProstatica"}}
      {{onChange clearFields="reactivosAntigenoProstatico"}}
    {{/field}}
    {{#field id="resultadoEspirometria"}}
      {{onChange clearFields="limiteInferiorNormalidadEspirometria,resultadoPruebaEspirometria"}}
    {{/field}}
  {{/section}}
{{/define}}