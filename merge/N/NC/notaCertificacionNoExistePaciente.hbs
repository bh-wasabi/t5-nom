{{#define id="notaCertificacionNoExistePaciente"}}
  {{action 
    id="afectar"
    confirm="=calc.contieneMalaPalabra(base.paciente.base.clave)||(base.paciente.base.tipoAtencion!='consultaExterna'&&base.paciente.base.genero=='intersexual')"
    confirmWidth="400"
    confirmHeight="400"
    confirmMessage="=fn('confirmarRegistroPaciente', base)"
  }}

  {{#section id="base"}}
    {{validator type="expr" validIf="=verificacionBusqueda=='si'" message="Falta Certificar"}}
  {{/section}}
{{/define}}