{{#define id="seulInterconsulta"}}
  {{#section id="base"}}
    {{#field id="interconsulta"}}
      {{#editor}}
        {{param persona="=_mainDoc.persona.id"}}
        {{param episodio="_mainDoc.persona.episodio"}}
      {{/editor}}
      {{#onChange}}
        {{set claveServicioInterconsultante="=base.interconsulta.base.servicioInterconsultante"}}
        {{set otroServicioInterconsultante="=base.interconsulta.base.otroServicioInterconsultante"}}
        {{set claveServicioInterconsultado="=base.interconsulta.base.servicioInterconsultado"}}
        {{set otroServicioInterconsultado="=base.interconsulta.base.otroServicioInterconsultado"}}
        {{set especialidadMedicoInterconsultado="=base.interconsulta.base.especialidadMedicoInterconsultado"}}
        {{set paisMedicoInterconsultado="=base.interconsulta.base.paisMedicoInterconsultado"}}
        {{set curpMedicoInterconsultado="=base.interconsulta.base.curpMedicoInterconsultado"}}
        {{set medicoInterconsultado="=base.interconsulta.base.medicoInterconsultado"}}
        {{set nombreMedicoInterconsultado="=base.interconsulta.base.nombreMedicoInterconsultado"}}
        {{set apellidoPaternoMedicoInterconsultado="=base.interconsulta.base.apellidoPaternoMedicoInterconsultado"}}
        {{set apellidoMaternoMedicoInterconsultado="=base.interconsulta.base.apellidoMaternoMedicoInterconsultado"}}
        {{set cedulaMedicoInterconsultado="=base.interconsulta.base.cedulaMedicoInterconsultado"}}
        {{set excedeLimiteEdad="=moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')>=18&&calc.in(base.interconsulta.base.servicioInterconsultado,fn('servicioEspecialidadPediatria'))||(_parentDoc.persona.genero==='mujer'&&calc.in(base.interconsulta.base.servicioInterconsultado,fn('servicioEspecialidadGinecoobstetricia'))&&(moment(_parentDoc.base.fechaEgreso).diff(_parentDoc.persona.fechaNacimiento,'years')<9||moment(_parentDoc.base.fechaInterconsulta).diff(_parentDoc.persona.fechaNacimiento,'years')>59))?'SI':'NO'"}}
      {{/onChange}}
    {{/field}}
  {{/section}}
{{/define}}
