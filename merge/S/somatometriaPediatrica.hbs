{{#define id="somatometriaPediatrica"}}
  {{#section id="base"}}
    {{field id="imc" value="=calc.round(peso/(talla*talla),2)*10"}}
    {{field id="superficieCorporal" value="=calc.round(0.007184*Math.pow(talla,0.725)*Math.pow(peso,0.425),2)"}}
    {{!-- 9 Feb 2024 --}}
    {{validator type="expr" validIf="=!@talla||(@talla==999||(@talla>=30&&@talla<=220))" message="Talla Incorrecta"}}
  {{/section}}
{{/define}}