{{#define id="atencionPrehospitalariaLesionSeul"}}
  {{#section id="base"}}
    {{validator type="expr" validIf="=!@tipoDroga||@tipoDroga.length<4" message="Máximo 3 Drogas"}}
    {{validator type="expr" notValidIf="=@tipoDroga.length>1&&(calc.in('ninguna',@tipoDroga)||calc.in('seIgnora',@tipoDroga))" message="Drogas Incorrectas"}}
    {{validator type="expr" validIf="=tipoDroga.length<6" message="Máximo 5 Drogas"}}
    {{validator type="expr" notValidIf="=@tiempoTranscurridoAtencionHospital=='00:00'" message="Tiempo Transcurrido Inválido (Mayor que 00:00)"}}
  {{/section}}
{{/define}}