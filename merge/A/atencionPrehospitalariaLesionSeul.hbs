{{#define id="atencionPrehospitalariaLesionSeul"}}
  {{#section id="base"}}
    {{validator type="expr" validIf="=!@tipoDroga||@tipoDroga.length<4" message="Máximo 3 Drogas"}}
    {{validator type="expr" notValidIf="=@tipoDroga.length>1&&(calc.in('ninguna',@tipoDroga)||calc.in('seIgnora',@tipoDroga))" message="Drogas Incorrectas"}}
    {{validator type="expr" validIf="=tipoDroga.length<6" message="Máximo 5 Drogas"}}
    {{validator type="expr" validIf="=!@tiempoTranscurridoAtencionHospital||calc.matchIfPattern(/^([0-9]|[0-3][0-9]|4[0-7]):([0-5][0-9]|00)$/, @tiempoTranscurridoAtencionHospital)||@tiempoTranscurridoAtencionHospital=='48:00'||@tiempoTranscurridoAtencionHospital=='99:99'" message="Verificar Tiempo, Formato HH:MM"}}
    {{validator type="expr" notValidIf="=@tiempoTranscurridoAtencionHospital=='00:00'" message="Tiempo Mínimo: 00:01"}}
  {{/section}}
{{/define}}