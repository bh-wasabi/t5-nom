{{#define id="atencionMedicaLesionSeul"}}
  {{#section id="base"}}
    {{field id="esOtroServicioAtencion" type="expr" value="=@servicioAtencionLesion==='otro'"}}
    {{field id="esOtraAreaAfectada" type="expr" value="=@areaAnatomica==='otros'"}}
    {{field id="esOtraConsecuencia" type="expr" value="=@consecuenciaLesion==='otra'"}}
    {{validator type="expr" validIf="=!calc.in(@consecuenciaLesion,['aborto','embarazo'])||esMujer&&calc.in(@consecuenciaLesion,['aborto','embarazo'])&&(edad>=9&&edad<=49)" message="Error en Consecuencia Lesion"}}
    {{validator type="expr" validIf="=@tipoAtencionLesion.length<6" message="Máximo 5 Tipo Atención"}}
    {{validator type="expr" validIf="=!@descripcionAfeccion||calc.matchIfPattern('^[A-z].{1,249}$', @descripcionAfeccion)" message="Verificar Descripción"}}
    {{validator type="expr" validIf="=!calc.in(calc.lowerCase(calc.normalize(@especifiqueServicioAtencion)),['consulta externa','hospitalizacion','urgencias','servicio especializado violencia','otro'])" message="Especificar Servicio Atención Inválido"}}
    {{validator type="expr" validIf="=!calc.in(calc.lowerCase(calc.normalize(@otraAreaAfectada)),['cabeza','cara','region ocular','cuello','columna vertebral','extremidades superiores','mano','torax','espalda y/o gluteos','espalda','gluteos','abdomen','pelvis','region genital','extremidades inferiores','pies','multiples','otros','no hubo lesion'])" message="Otra Área Afectada Inválida"}}
    {{validator type="expr" validIf="=!calc.in(calc.lowerCase(calc.normalize(@especifiqueConsecuencia)),['laceracion / abracion','laceracion','abracion','aplastamiento','cicatrices','depresion','contusion / magullamiento','contusion','magullamiento','congelamiento','aborto','trastornos de ansiedad estres postraumatico','quemadura / corrosion','quemadura','corrosion','asfixia','embarazo','trastornos psiquiatricos','luxacion / esguince','luxacion','esguince','herida','infeccion transmision sexual','multiple','amputacion / avulsion','amputacion','avulsion','fractura','defuncion','malestar emocional','transtorno del estado de animo','otra','otro'])" message="Especifique Consecuencia Inválida"}}
  {{/section}}
{{/define}}