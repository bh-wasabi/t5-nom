    {{column field="cantidad" width="130" defaultValue="1"}}
  {{/grid}}
  {{#grid id="productos" section="productos" allowRemove="true" allowSort="true" keyField="articulo" allowInsert="true"}}
    {{column field="articulo" width="150"}}
    {{column field="descripcion" width="300" readOnly="true"}}
    {{column field="tipoArticulo" width="150" readOnly="true"}}
    {{column field="cantidad" width="80" defaultValue="1"}}
