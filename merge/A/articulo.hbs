{{#define id="articulo"}}
  {{param name="=_params._tipoArticulo=='Página'?_params._name:(_params._tipoActivoFijo||_params._tipoArticulo||'Artículo')"}}
  {{param removeIfEmptySection="materiales"}}
  {{param removeIfEmptyField="articulo"}}

  {{action id="eliminar" hide="true"}}
  {{!-- {{action id="altaMedicamento" source="notaAltaArticulo" subType="medicamento" subTypeName="Medicamento" label="Alta Medicamento" hide="true" type="add-subdoc" visibleMode="close" color="indigo" isBrowserAction="true" onlyPost="true"}}   --}}
[.#if esSIC.]
  {{action id="subdoc" condition="=base.tipoArticulo=='activoFijo'" type="menu" menu="subdoc" label="Plano" color="grey" visibleMode="close"}}
  {{#menu id="subdoc" color="grey"}}
    {{item action="bim" text="BIM"}}
    {{item action="planta" text="Planta"}}
  {{/menu}}  
  {{#action id="bim" hide="true" type="bim360" label="BIM" title="=calc.concat('BIM', _items[0].persona.planta)" bim="=_items[0].persona.bim" color="grey" visibleMode="close" fromSource="persona" fromView="datosUbicacion"}}
    {{param ubicacion="=base.ubicacion"}}
  {{/action}}
  {{#action id="planta" hide="true" type="openPreview" label="Planta" title="Planta" color="grey" visibleMode="close" fromSource="persona" fromView="datosUbicacion"}}
    {{param ubicacion="=base.ubicacion"}}
    {{link title="=calc.concat('Planta', _items[0].persona.planta)" href="=fn('urlPlanta', _items[0].persona.planta)"}}
  {{/action}}
  {{#action id="subdoc2" condition="=base.datosActivoFijo.base.urlCompra" type="openPreview" label="Factura Compra" title="Factura Compra" color="grey" visibleMode="close"}}
    {{link href="=base.datosActivoFijo.base.urlCompra" title="=calc.concat('Factura Compra',base.datosActivoFijo.base._facturaCompra)" name="Factura Compra"}}
  {{/action}}
  {{#action id="tool" condition="=base.tipoArticulo=='activoFijo'" label="Ver Planeación" type="view-scheduler" color="cyan" visibleMode="close"}}
    {{scheduler type="plan" lastDate="=ultimoMantenimiento" description="=calc.concatEnter(@_procedimiento, @comentarios)" text="=calc.concatDash(@_subTipoSolicitud, @_motivo)" time="=periodicidad" timeUnit="=tiempo" timeTable="=horario" startDayHour="0" endDayHour="24" cellDuration="60" resourceItems="=mantenimiento" resourceText="=_subTipoSolicitud" views="month,week,day" currentView="month"}}
  {{/action}}
[.else.]
  {{#action id="tool" condition="=base.tipoArticulo=='activoFijo'" label="Ver Planeación" type="view-scheduler" color="cyan" visibleMode="close"}}
    {{scheduler type="plan" startDayHour="=base.horaInicioDia" endDayHour="=base.horaFinDia" cellDuration="30" resourceSource="planAgenda" resourceView="agendasActivas" resourceField="schedule" views="month,week,day" currentView="day"}}
  {{/action}}
  {{#action id="tool2" condition="=base.tipoArticulo=='activoFijo'" label="Ver Agenda" type="view-scheduler" btnSolid="false" color="green" visibleMode="close"}}
    {{scheduler type="busy" startDayHour="=base.horaInicioDia" endDayHour="=base.horaFinDia" cellDuration="30" resourceSource="planAgenda" resourceView="agendasActivas" resourceField="schedule" views="month,week,day" currentView="day"}}
  {{/action}}
[./if.]    
  {{action id="tool3" condition="=true" type="menu" menu="tool3" label="Ver" color="grey" visibleMode="close"}}
  {{#menu id="tool3" color="grey"}}
    {{item action="verSolicitudes" text="Solicitudes"}}
    {{item action="verSolicitudesMantenimiento" text="Solicitudes Mantenimiento"}}
    {{item action="verRegistros" text="Registros"}}
  {{/menu}}  
  {{#action id="verRegistros" condition="=true" label="Ver Registros" type="sub-link" btnSolid="false" color="green" visibleMode="close"}}
    {{#link href="'/nota/lista'" title="=calc.concat('Registros', _name)" name="=calc.concat('Registros', _name, '-', base.codigo)"}}
      {{param referencia="=base.codigo"}}
    {{/link}}
  {{/action}}
  {{#action id="verSolicitudes" label="Ver Solicitudes" type="sub-link" btnSolid="false" color="green" visibleMode="close" condition="=base.tipoArticulo!='procedimientoMantenimiento'"}}
    {{ask field="desde" type="date" label="Desde" value="=moment().add(-7,'day').format('YYYY-MM-DD')"}}
    {{ask field="hasta" type="date" label="Hasta" value="=moment().format('YYYY-MM-DD')"}}
    {{#link href="'/solicitud/lista'" title="=calc.concat('Solicitudes', _name)" name="=calc.concat('Solicitudes', _name, '-', base.codigo, '(Desde:', moment(desde).format('DD/MMM/YYYY'), '-', 'Hasta:', moment(hasta).format('DD/MMM/YYYY')+')')"}}
      {{param articulo="=_id"}}
    {{/link}}
  {{/action}}
  {{#action id="verSolicitudesMantenimiento" label="Ver Solicitudes Mantenimiento" type="sub-link" btnSolid="false" color="green" visibleMode="close" condition="=base.tipoArticulo=='procedimientoMantenimiento'"}}
    {{#link href="'/solicitud/lista'" title="=calc.concat('Solicitudes Mantenimiento', _name)" name="=calc.concat('Solicitudes Mantenimiento', _name, '-', base.codigo)"}}
      {{param articuloMantenimiento="=_id"}}
    {{/link}}
  {{/action}}
  {{#action id="verRegistroConsumoElectrico" condition="=true" label="Bitácora Consumo Eléctrico" type="sub-link" btnSolid="false" color="grey" visibleMode="close"}}
    {{#link href="'/nota/lista?tipo=notaRegistroConsumoElectrico'" title="=calc.concat('Bitácora Consumo Eléctrico', _name)" name="=calc.concat('Bitácora Consumo Eléctrico', _name, '-', base.codigo)"}}
      {{param referencia="=base.codigo"}}
    {{/link}}
  {{/action}}
  {{action id="ver2" type="openFolder" condition="=adjuntos" items="=adjuntos" label="Adjuntos" color="grey" visibleMode="close"}}
  [.#if esSIC.]  
  {{action id="copiar" label="Copiar..." condition="=true" confirm="true" confirmMessage="¿Desea Copiar el Artículo?" btnSolid="false" color="grey"}}
  [./if.]
  {{!-- {{action id="compartir" type="share" label="Compartir" color="grey" icon="share" visibleMode="close"}} --}}
  {{!-- {{action id="tool" type="attachTemplate" label="Adjuntar Plantilla..." color="grey" visibleMode="close" condition="=true"}} --}}

  {{#section id="base"}}
[.#if esERP.]
    {{field id="nombre" type="expr" value="=calc.concatValueKey(@descripcion, @codigo)"}}
[.else.]
    {{field id="nombre" type="expr" value="=@descripcion"}}
[./if.]        
[.#if esSIC.]
    {{field id="nombre2" type="expr" value="=calc.concat(@_servicio, '-', @descripcion, '('+@codigo+')')"}}
[./if.]    
    {{field id="tipoArticulo" defaultValue="=_params.tipoArticulo" readOnly="true"}}
    {{field id="tipoActivoFijo" defaultValue="=_params.tipoActivoFijo" readOnly="true"}}
    {{field id="esMaterial" type="expr" value="=tipoArticulo==='material'"}}
    {{field id="esMaterialCeye" type="expr" value="=tipoArticulo==='materialCeye'"}}
    {{field id="esInstrumento" type="expr" value="=tipoArticulo==='instrumento'"}}
    {{field id="esTipoActivo" type="expr" value="=tipoArticulo==='tipoActivo'"}}
    {{field id="esTipoEquipo" type="expr" value="=tipoArticulo==='tipoEquipo'"}}
    {{field id="esActivoFijo" type="expr" value="=tipoArticulo==='activoFijo'"}}
    {{field id="esActivoInactivo" type="expr" value="=esActivoFijo&&estatus=='inactivo'"}}
    {{field id="puedeSustituir" type="expr" value="=esActivoFijo&&estatus=='inactivo'"}}
    {{field id="esActivoSustituido" type="expr" value="=puedeSustituir&&sustituido=='si'"}}
    {{field id="esKit" type="expr" value="=calc.in(tipoArticulo, ['kit','kitMedico','menu','pagina','mezcla'])"}}
    {{field id="esKitMedico" type="expr" value="=tipoArticulo==='kitMedico'"}}
    {{field id="esMenu" type="expr" value="=tipoArticulo==='menu'"}}
    {{field id="esMenuDia" type="expr" value="=tipoArticulo==='menuDia'"}}
    {{field id="esPagina" type="expr" value="=tipoArticulo==='pagina'"}}
    {{field id="esMedicamento" type="expr" value="=calc.in(tipoArticulo, ['medicamento','medicamentoGratuito'])"}}
    {{field id="esMedicamentoGratuito" type="expr" value="=tipoArticulo==='medicamentoGratuito'"}}
    {{field id="esSolucion" type="expr" value="=tipoArticulo==='solucion'"}}
    {{field id="esMedicamentoSolucion" type="expr" value="=calc.in(tipoArticulo, ['medicamento','medicamentoGratuito','solucion'])"}}
    {{field id="esMezcla" type="expr" value="=tipoArticulo==='mezcla'"}}
    {{field id="esProcedimiento" type="expr" value="=tipoArticulo==='procedimiento'"}}
    {{field id="esProcedimientoMantenimiento" type="expr" value="=tipoArticulo==='procedimientoMantenimiento'"}}
    {{field id="esMultiservicio" type="expr" value="=tipoArticulo==='multiservicio'"}}
    {{field id="esPlanCompras" type="expr" value="=tipoArticulo==='planCompras'"}}
    {{field id="esConceptoGasto" type="expr" value="=tipoArticulo==='conceptoGasto'"}}
    {{field id="esConceptoNomina" type="expr" value="=tipoArticulo==='conceptoNomina'"}}
    {{field id="esRonda" type="expr" value="=tipoArticulo=='ronda'"}}
    {{field id="esLlave" type="expr" value="=tipoArticulo=='llave'"}}
    {{field id="esTarjetaAcceso" type="expr" value="=tipoArticulo=='tarjetaAcceso'"}}
    {{field id="esSeguridad" type="expr" value="=esLlave||esTarjetaAcceso||esRonda"}}
    {{field id="esOtroServicio" type="expr" value="=tipoArticulo==='otroServicio'"}}
    {{field id="esCurso" type="expr" value="=tipoArticulo=='curso'"}}
    {{field id="esMoi" type="expr" value="=esActivoFijo&&tipoActivoFijo==='moi'"}}
    {{field id="esControlVectores" type="expr" value="=esActivoFijo&&tipoActivoFijo==='controlVectores'"}}
    {{field id="esOtroTipoVector" type="expr" value="=escontrolVectores&&tipoVector=='otros'"}}
    {{field id="esVehiculo" type="expr" value="=esActivoFijo&&tipoActivoFijo=='vehiculo'"}}
    {{!-- {{field id="esEvaluacion" type="expr" value="=calc.in(tipoArticulo, ['competencia'])"}} --}}
    {{field id="esCompetencia" type="expr" value="=tipoArticulo==='competencia'"}}
    {{field id="esServicio" type="expr" value="=(esProcedimiento||esProcedimientoMantenimiento||esOtroServicio||esMultiservicio||esPlanCompras||esKit||esMenuDia||esSeguridad||esConceptoGasto||esConceptoNomina||esCurso)&&!esMenu"}}
    {{field id="noEsServicio" type="expr" value="=!esServicio"}}
    {{field id="esBien" type="expr" value="=!esServicio&&!esActivoFijo&&!esTipoActivo&&!esTipoEquipo&&!esCompetencia"}}
    {{field id="esProducto" type="expr" value="=calc.in(tipoArticulo, ['producto','productoCafeteria'])"}}
    {{field id="esProductoCafeteria" type="expr" value="=tipoArticulo==='productoCafeteria'"}}
    {{field id="esRopa" type="expr" value="=tipoArticulo==='ropa'"}}
    {{field id="sePuedeProducir" type="expr" value="=calc.in(tipoArticulo, ['material','ropa'])"}}
    {{field id="seProduce" type="expr" value="=calc.isTrue(base.produccion)"}}
    {{field id="tieneIvaVenta" type="expr" value="=esBien||esProcedimiento||esOtroServicio||esTipoActivo"}}
    {{field id="tieneIvaCompra" type="expr" value="=esBien||esConceptoGasto||esActivoFijo||esTipoActivo"}}
    {{field id="tieneRetenciones" type="expr" value="=esConceptoGasto"}}
    {{field id="tieneDatosMedicamento" type="expr" value="=esMedicamento||esSolucion"}}
    {{field id="tieneUbicacion" type="expr" value="=esActivoFijo||esMultiservicio||esPlanCompras"}}
    {{field id="tieneEstablecimiento" type="expr" value="=tieneUbicacion||esRopa"}}   
    {{field id="tienePersonal" type="expr" value="=esLlave||esTarjetaAcceso"}}
    {{field id="tieneEspecialidad" type="expr" value="=esMoi"}}
    {{field id="tieneMateriales" type="expr" value="=sePuedeProducir&&seProduce"}}
    {{field id="tieneDeducibilidad" type="expr" value="=esActivoFijo||esConceptoGasto"}}
    {{field id="tieneClasificadores" type="expr" value="=esBien||esCompetencia"}}
    {{field id="tieneCuentaMayor" type="expr" value="=esConceptoGasto||esConceptoNomina||esTipoActivo"}}
    {{field id="esPorcentajeDeducibilidad" type="expr" value="=tieneDeducibilidad&&tipoDeducibilidad=='porcentaje'"}}
    {{field id="esTopeDeducibilidad" type="expr" value="=tieneDeducibilidad&&tipoDeducibilidad=='tope'"}}
    {{field id="costoEstandar" type="calc" value="=calc.sumRef(hojaCostos, 'costoTotal')"}}
    {{#field id="cuentaContable"}}
      {{#onChange}}
        {{set cuenta="=base.cuenta"}}
      {{/onChange}}
    {{/field}}
    {{#field id="cuentaContableDepreciacionActivos"}}
      {{#onChange}}
        {{set cuentaDepreciacionActivos="=base.cuenta"}}
      {{/onChange}}
    {{/field}}
    {{#field id="cuentaContableDepreciacionGastos"}}
      {{#onChange}}
        {{set cuentaDepreciacionGastos="=base.cuenta"}}
      {{/onChange}}
    {{/field}}
    {{#field id="subTipoActivoFijo"}}
      {{#editor}}
        {{param tipoActivoFijo="=tipoActivoFijo"}}
      {{/editor}}
    {{/field}}
    {{#field id="sustituto"}}
      {{#editor}}
        {{param notId="=_id"}}
        {{param tipoArticulo="=tipoArticulo"}}
      {{/editor}}      
    {{/field}}
    {{#field id="servicio" defaultValue="=_params.servicio"}}
      {{onChange clearFields="ubicacionAbre,activoFijoAbre"}}
[.#if esSIC.]      
      {{#editor}}
        {{param condition="=fn('servicioDelArticulo', base.tipoArticulo, base.tipoActivoFijo)!=null" partOf="=fn('servicioDelArticulo', base.tipoArticulo, base.tipoActivoFijo)"}}
      {{/editor}}
[./if.]      
    {{/field}}
    {{#field id="ubicacion"}}
      {{#editor}}
[.#if esSIC.]    
        {{param establecimiento="=establecimiento"}}
[./if.]      
        {{!-- {{#onChange}}
          {{set area="=persona.area"}}
          {{set criticidadZona="=persona.criticidadZona"}}
        {{/onChange}} --}}
      {{/editor}}
    {{/field}}
    {{#field id="ubicaciones"}}
      {{editor itemTemplate="nombre2" display="persona.nombre2"}}
    {{/field}}
    {{#field id="activosFijos"}}
      {{editor itemTemplate="nombre2" display="base.nombre2"}}
    {{/field}}
    {{!-- {{#field id="partidaPresupuestal"}}
      {{onChange clearFields="cucop"}}
    {{/field}}
    {{#field id="cucop"}}
      {{#editor}}
        {{param partOf="=partidaPresupuestal"}}
      {{/editor}}      
    {{/field}} --}}
    {{!-- {{#field id="subTipoActivoFijo"}}
      {{#editor}}
        {{param partOf="=tipoActivoFijo"}}
      {{/editor}}      
    {{/field}} --}}
    {{#field id="especialidad"}}
      {{#editor}}
        {{param partOf="=tipoActivoFijo"}}
        {{onChange clearFields="subEspecialidad"}}
      {{/editor}}      
    {{/field}}
    {{#field id="subEspecialidad"}}
      {{#editor}}
        {{param partOf="=especialidad"}}
      {{/editor}}      
    {{/field}}
    {{#field id="cliente"}}
      {{#editor}}
        {{param tipoExpediente="='cliente'"}}
      {{/editor}}      
    {{/field}}
    {{field id="esAlmacenProduccion" type="expr" value="=seProduce&&tipoAlmacenProduccion=='almacen'"}}
    {{field id="esUbicacionProduccion" type="expr" value="=seProduce&&tipoAlmacenProduccion=='ubicacion'"}}
    {{field id="esPersonaProduccion" type="expr" value="=seProduce&&tipoAlmacenProduccion=='persona'"}}
    {{field id="esServicioProduccion" type="expr" value="=seProduce&&tipoAlmacenProduccion=='servicio'"}}
    {{#field id="ubicacionProduccion"}}
      {{#editor}}
        {{param tipoExpediente="='ubicacion'"}}
      {{/editor}}
    {{/field}}
    {{#field id="personaProduccion"}}
      {{#editor}}
        {{param tipoExpediente="='paciente'"}}
      {{/editor}}
    {{/field}}
    {{#field id="servicioProduccion"}}
      {{#editor}}
        {{param tipoExpediente="='servicio'"}}
      {{/editor}}
    {{/field}}
    {{#field id="sustituidoPor"}}
      {{#editor}}
        {{param notId="=_id"}}
        {{param tipoActivoFijo="=tipoActivoFijo"}}
        {{param estatus="='inactivo'"}}
      {{/editor}}
    {{/field}}
  {{/section}}
  {{#section id="hojaCostos"}}
    {{field id="costoTotal" type="calc" value="=cantidad*costoUnitario"}}
    {{onChange forceRecalc="true"}}
  {{/section}}

  {{#transform id="copiarActivoFijo"}}
    {{#update section="base"}}
      {{set articulo="_id"}}
      {{set _articulo="_name"}}
      {{set codigo="base.codigo"}}
      {{set descripcion="base.descripcion"}}
    {{/update}}
    {{update section="niveles" value="=calc.tagsToArray(base.datosActivoFijo.base.niveles, 'nivel')"}}
  {{/transform}}

  {{#view id="todo"}}
    {{#find limit="1"}}
      {{filter condition="=articulo" field="_id" eq="=articulo" isObjectId="true"}}
      {{filter condition="=codigo" field="base.codigo" eq="=codigo"}}
    {{/find}}
  {{/view}}

  {{#view id="descripcion"}}
    {{#find}}
      {{include field="base.descripcion"}}
      {{include field="base.unidadExistencia"}}
      {{include field="base._unidadExistencia"}}
      {{include field="base.tipoArticulo"}}
      {{include field="base._tipoArticulo"}}
      {{include field="base.servicio"}}
      {{include field="base.datosRopa"}}
      {{include field="base.categoria"}}
[.#if esSIC.]      
      {{include field="tipoActivo.campo8"}}
[./if.]      
    {{/find}}
  {{/view}}

  {{#view id="articulosDeLaCategoria"}}
    {{#find limit="-1" flatten="true"}}
      {{include field="base.codigo" as="codigo"}} 
      {{filter field="base.categoria" eq="=categoria"}}
    {{/find}}
  {{/view}}

  {{#view id="listaArticulos"}}
    {{#find limit="-1"}}
      {{include field="base.codigo"}} 
      {{include field="base.descripcion"}}
      {{include field="base.datosRopa"}}
      {{include field="base.tipoArticulo"}}
      {{include field="base.multidosis"}}
      {{include field="base.datosRopa.base._tipoPrenda"}}
      {{include field="base.partidaPresupuestal"}}
      {{include field="base.cucop"}}
      {{include field="base.unidadExistencia"}}
      {{include field="base.articuloProveedor"}}
      {{sort field="base.codigo" direction="asc"}}
      {{filter condition="=codigo" field="base.codigo" in="=codigo"}}
      {{filter condition="=tipoArticulo" field="base.tipoArticulo" in="=tipoArticulo"}}
      {{filter condition="=tipoActivoFijo" field="base.tipoActivoFijo" in="=tipoActivoFijo"}}
    {{/find}}
  {{/view}}

  {{#view id="materialesDelServicio"}}
    {{#find limit="-1" flatten="true"}}
      {{include field="base.codigo" as="codigo"}} 
      {{filter field="base.tipoArticulo" eq="='material'"}}
      {{filter condition="=servicio" field="base.servicio" in="=servicio"}}
    {{/find}}
  {{/view}}

  {{#view id="listaActivos"}}
    {{#find limit="-1"}}
      {{include field="base"}}
      {{sort field="base.codigo" direction="asc"}}
      {{filter condition="=tipoArticulo" field="base.tipoArticulo" in="=tipoArticulo"}}
      {{filter condition="=tipoActivoFijo" field="base.tipoActivoFijo" in="=tipoActivoFijo"}}
      {{filter condition="=servicio" field="base.servicio" in="=servicio"}}
    {{/find}}
  {{/view}}

  {{#view id="listaDisponibilidadActivos"}}
    {{#find limit="-1"}}
      {{include field="base"}}
      {{sort field="base.codigo" direction="asc"}}
      {{filter condition="=tipoArticulo" field="base.tipoArticulo" in="=tipoArticulo"}}
      {{filter condition="=tipoActivoFijo" field="base.tipoActivoFijo" in="=tipoActivoFijo"}}
    {{/find}}
    {{calc field="tiempoTotal" type="expr" value="=base.datosActivoFijo.base.disponibilidadProgramadaSemestral.base.minutos"}}
    {{calc field="codigo" type="expr" value="=base.codigo"}}
    {{#join2 source="solicitud" view="tiempoNoDisponibilidad" key="codigo" id="codigo" as="tiempoNoDisponibilidad" value="minutos"}}
      {{param codigo="=codigo"}}
    {{/join2}}
    {{calc2 field="disponibilidadActual" type="calc" value="=100-(@tiempoNoDisponibilidad||0)/tiempoTotal"}}
  {{/view}}

  {{#view id="sugerirArticulos"}}
    {{#find limit="-1" flatten="true"}}
      {{include field="_id" as="articulo"}}
      {{include field="_name" as="_articulo"}}
      {{include field="base.codigo" as="codigo"}}
      {{include field="base.descripcion" as="descripcion"}}
      {{include field="base.categoria" as="categoria"}}
      {{include field="base.grupo" as="grupo"}}
      {{include field="base.familia" as="familia"}}
      {{include field="base.unidadEvaluacion" as="unidadEvaluacion"}}
      {{include field="base.ejercicio" as="ejercicio"}}
      {{include field="base.ponderacion" as="ponderacion"}}
      {{sort field="base.codigo" direction="asc"}}
      {{filter field="base.estatus" eq="='activo'"}}
      {{filter condition="=tipoArticulo" field="base.tipoArticulo" in="=tipoArticulo"}}
      {{filter condition="=ejercicio" field="base.ejercicio" in="=ejercicio"}}
    {{/find}}
  {{/view}}

  {{#view id="sugerirInsumosUtilizados"}}
    {{#find limit="-1" flatten="true"}}
      {{include field="_id" as="articulo"}}
      {{include field="_name" as="_articulo"}}
      {{include field="base.codigo" as="codigo"}}
      {{include field="base.descripcion" as="descripcion"}}
      {{sort field="base.codigo" direction="asc"}}
      {{filter field="base.estatus" eq="='activo'"}}
      {{filter field="base.tipoArticulo" in="='material'"}}
      {{filter condition="=servicio" field="base.servicio" in="=servicio"}}
    {{/find}}
  {{/view}}

  {{#view id="sugerirCompetencias"}}
    {{#find limit="-1" flatten="true"}}
      {{include field="_id" as="articulo"}}
      {{include field="_name" as="_articulo"}}
      {{include field="base.codigo" as="codigo"}}
      {{include field="base.descripcion" as="descripcion"}}
      {{include field="base.categoria" as="categoria"}}
      {{!-- {{include field="base.grupo" as="grupo"}} --}}
      {{!-- {{include field="base.familia" as="familia"}} --}}
