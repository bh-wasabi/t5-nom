  {{#grid id="componentes" section="componentes" allowRemove="true" allowSort="true" keyField="articulo" allowInsert="true"}}
    {{column field="articulo" width="200"}}
    {{column field="descripcion" width="400" readOnly="true"}}
    {{column field="tipoArticulo" width="150" readOnly="true"}}
    {{column field="cantidad" width="80" defaultValue="1"}}
  {{/grid}}
  {{#grid id="hojaCostos" section="hojaCostos" allowRemove="true" allowSort="true" keyField="tipoInsumo" allowInsert="true"}}
    {{column field="tipoInsumo" width="200"}}
    {{column field="descripcion" width="400"}}
    {{column field="unidad" width="120"}}
    {{column field="cantidad" width="80" defaultValue="1"}}
    {{column field="costoUnitario" width="100" format="currency"}}
  {{/grid}}
  {{#grid id="menuDia" section="menuDia" allowRemove="true" allowSort="true" keyField="regimenAlimenticio" allowInsert="true"}}
    {{column field="tipoPaciente" width="150"}}
    {{column field="regimenAlimenticio" width="180"}}
    {{column field="desayuno" width="180"}}
    {{column field="colacionAM" width="180"}}
    {{column field="almuerzo" width="180"}}
    {{column field="colacionPM" width="180"}}
    {{column field="cena" width="180"}}
    {{column field="colacionNoche" width="180"}}
    {{column field="colacionExtra" width="180"}}
  {{/grid}}
  {{#grid id="mantenimiento" section="mantenimiento" allowRemove="true" allowSort="true" keyField="motivo" allowInsert="true"}}
    {{column field="subTipoSolicitud" width="150"}}
    {{column field="procedimiento" width="150"}}
    {{column field="motivo" width="150"}}
    {{column field="periodicidad" width="70"}}
    {{column field="tiempo" width="90"}}
    {{column field="horario" width="100" validateHours="true"}}
    {{column field="diasAtencion" width="100"}}
    {{column field="ultimoMantenimiento" width="160" validateDate="true"}}
    {{column field="siguienteMantenimiento" width="170" validateDate="true"}}
    {{column field="autoAsignacion" width="150"}}
    {{column field="comentarios" width="150"}}
  {{/grid}}
  {{#grid id="adjuntos" section="adjuntos" allowRemove="true" allowSort="true" sortBy="orden, fecha"}}
