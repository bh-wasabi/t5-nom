  {{#transform id="copiarConcepto" items="aplicacion" where="=importe"}}
    {{set _name="=_name"}}
    {{set satIva="=(@sat&&sat.iva)||0"}}
    {{set _satIva="=(@sat&&sat._iva)||'IVA 0%'"}}
    {{set satCodigo="=(@sat&&sat.satCodigo)||'85121600'"}}
    {{set _satCodigo="=(@sat&&sat._satCodigo)||'Servicios médicos de doctores especialistas'"}}
    {{set satUnidad="=(@sat&&sat.satUnidad)||'C62'"}}
    {{set _satUnidad="=(@sat&&sat._satUnidad)||'Unidad'"}}
    {{set importe="=importe"}}
    {{set cantidad="=(@cantidad||1)"}}
    {{set valorUnitario="=fn('importeSinIva', importe, (@sat&&sat.iva)||0)/(@cantidad||1)"}}
    {{set importeIva="=importe-fn('importeSinIva', importe, (@sat&&sat.iva)||0)"}}
    {{set subTotal="=fn('importeSinIva', importe, (@sat&&sat.iva)||0)||importe"}}
  {{/transform}}

  {{#transform id="copiarConceptoAnticipo" items="anticipo"}}
    {{set _name="=_name"}}
    {{set satIva="=Number(0)"}}
    {{set _satIva="='IVA 0%'"}}
    {{set satCodigo="='85121600'"}}
    {{set _satCodigo="='Servicios médicos de doctores especialistas'"}}
    {{set satUnidad="='C62'"}}
    {{set _satUnidad="='Unidad'"}}
    {{set importe="=base.totalCobrado"}}
    {{set cantidad="=Number(1)"}}
    {{set valorUnitario="=base.totalCobrado"}}
    {{set importeIva="=Number(0)"}}
    {{set subTotal="=base.totalCobrado"}}
  {{/transform}}

  {{#function id="listaEstatus"}}
    {{define type="param" id="tipo"}}
    {{when tipo="activos" return="nuevo,instalar,continuar,activo-controlado,activo-no-controlado,no-realizado"}}
    {{when tipo="inactivos" return="suspender,eliminar,remover,inactivo,rechazado,descartado,cancelado,sinExistencia"}}
  {{/function}}

