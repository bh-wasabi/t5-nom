  {{#function id="folioServiceName"}}
    {{define type="param" id="name"}}
    {{define type="param" id="created"}}
    {{default return="=(!@created.service||@created.service=='ver')?calc.concat(@name, @created.folio):calc.concat(@name, (@created.board==='telemedicina')?@created._board:'', @created._service, @created.folio)"}}
  {{/function}}

  {{#function id="dynamicName"}}
    {{define type="param" id="name"}}
    {{define type="param" id="created"}}
    {{default return="=calc.concat(@created.name||@name, @created.folio)"}}
  {{/function}}

  {{#function id="dynamicServiceName"}}
    {{define type="param" id="name"}}
    {{define type="param" id="created"}}
    {{default return="=@created.subType?calc.concat(@created.name, @created.folio):fn('folioServiceName', name, created)"}}
  {{/function}}

  {{#function id="folioName"}}
    {{define type="param" id="name"}}
    {{define type="param" id="created"}}
    {{default return="=calc.concat(@name, (@created.board==='telemedicina')?@created._board:'', @created.folio)"}}
  {{/function}}

  {{#function id="notaTransenfermeriaName"}}
    {{define type="param" id="name"}}
    {{define type="param" id="created"}}
    {{define type="param" id="service"}}
    {{define type="param" id="horas"}}
    {{define type="param" id="_momento"}}
    {{when service="cirugia" return="=calc.concat(created.name||'Registro Transoperatorio Enfermería', @created.folio, calc.prefixIf(_momento, '- '))"}}
    {{when service="cirugiaAmbulatoria" return="=calc.concat(created.name||'Registro Transoperatorio Enfermería', @created.folio, calc.prefixIf(_momento, '- '))"}}
    {{when service="tococirugia" return="=calc.concat(created.name||'Registro Transoperatorio Enfermería', @created.folio, calc.prefixIf(_momento, '- '))"}}
    {{default return="=calc.concat(created.name||fn('folioServiceName', name, created), calc.printIf(horas, '('+horas+' Horas)'), calc.prefixIf(_momento, '- '))"}}
  {{/function}}

  {{#function id="llegaPacienteAtencionCamaCentral"}}
    {{define type="param" id="service"}}
    {{when service="cirugiaAmbulatoria" return="='cirugiaAmbulatoriaPre'"}}
    {{when service="tococirugia" return="='tococirugiaLabor'"}}
    {{default return="=service"}}
  {{/function}}

  {{#function id="iniciaAtencionCamaCentral"}}
    {{define type="param" id="service"}}
    {{when service="cirugiaAmbulatoria" return="='cirugiaAmbulatoriaTrans'"}}
    {{when service="tococirugia" return="='tococirugiaExpulsion'"}}
    {{default return="=service"}}
