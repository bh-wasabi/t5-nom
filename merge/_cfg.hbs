  {{#function id="_tipoMovimiento"}}
    {{define type="param" id="tipo"}}
    {{when tipo="notaFactura" return="Factura"}}
    {{when tipo="notaCancelacionFactura" return="Cancelación Factura"}}
    {{when tipo="notaCredito" return="Nota Crédito"}}
    {{when tipo="notaCancelacionCredito" return="Cancelación Nota Crédito"}}
    {{when tipo="notaCobroAnticipo" return="Cobro Anticipo"}}
    {{when tipo="notaCancelacionCobroAnticipo" return="Cancelación Cobro Anticipo"}}
    {{when tipo="notaAplicacionAnticipo" return="Aplicación Anticipo"}}
