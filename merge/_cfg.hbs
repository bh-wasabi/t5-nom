    {{define type="param" id="tipoEvento"}}
    {{when categoria="calidad" tipoEvento="eventoAdversoConDano" return="='seguridadPaciente'"}}
    {{when categoria="calidad" tipoEvento="eventoCentinela" return="='seguridadPaciente'"}}
