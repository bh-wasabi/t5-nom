{{#define id="procedimientoDiagnosticosTerapeuticosQuirurgicosAltaServicio"}}
  {{#section id="base"}}
    {{field id="esEnQuirofano" type="expr" value="=enQuirofanoOFuera=='en'"}}
    {{#field id="procedimientosRealizados"}}
      {{#editor display="procedimiento"}}
        {{param persona="=_mainDoc.persona.id"}}
        {{param tipoSolicitud="procedimiento,cirugia,tococirugia"}}
        {{#onChange clearFields="enQuirofanoOFuera,tipoAnestesia,descripcionProcedimiento"}}
          {{set procedimientoCie9="=fn('obtenerDatosProcedimiento',_parent.type,'procedimientoCie9',base)"}}
          {{set codigoCie9="=fn('obtenerDatosProcedimiento',_parent.type,'procedimientoCie9',base)"}}
          {{set limiteSexo="=fn('obtenerDatosProcedimiento',_parent.type,'limiteSexo',base)"}}
          {{set limiteInferior="=fn('obtenerDatosProcedimiento',_parent.type,'limiteInferior',base)"}}
          {{set limiteSuperior="=fn('obtenerDatosProcedimiento',_parent.type,'limiteSuperior',base)"}}
          {{set excedeLimiteEdad="=fn('obtenerDatosProcedimiento',_parent.type,'excedeLimiteEdad',base,_parentDoc)"}}
          {{set otrasValidaciones="=fn('obtenerDatosProcedimiento',_parent.type,'otrasValidaciones',base)"}}
        {{/onChange}}
      {{/editor}}
    {{/field}}
    {{#field id="enQuirofanoOFuera"}}
      {{onChange clearFields="cirujano,cedulaCirujano"}}
    {{/field}}
		{{#field id="cirujano"}}
			{{#onChange}}
				{{set cedulaCirujano="=_.first(prestadorServicios.datosEspecialidad).base.cedulaEspecialidad"}}
			{{/onChange}}
		{{/field}}
  {{/section}}
{{/define}}