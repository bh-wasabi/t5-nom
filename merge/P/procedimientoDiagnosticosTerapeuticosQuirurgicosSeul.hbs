{{#define id="procedimientoDiagnosticosTerapeuticosQuirurgicosSeul"}}
  {{#section id="base"}}
  	{{#onChange}}
  		{{#update}}
  			{{set cantidadProcedimientos="=procedimientosCirugiasCie9.length"}}
  		{{/update}}
  	{{/onChange}}
		{{#field id="cirujano"}}
			{{#onChange}}
				{{set cedulaCirujano="=_.first(prestadorServicios.datosEspecialidad).base.cedulaEspecialidad"}}
			{{/onChange}}
		{{/field}}
  	{{validator type="expr" validIf="=procedimientosCirugiasCie9.length<9" message="Máximo 8 Procedimientos"}}
		{{validator type="expr" notValidIf="=descripcionProcedimiento&&descripcionProcedimiento.length<2" message="Descripción Procedimiento Inválida"}}
  {{/section}}
{{/define}}