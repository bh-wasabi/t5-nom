              {{/if}}
              {{#if pic}}
              <span class="fondo-{{pic.color}}" style="font-size:12px;"><strong>&nbsp;&nbsp;{{pic.name}}&nbsp;&nbsp;</strong></span>
              {{/if}}
              {{#if pei}}
