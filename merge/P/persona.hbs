        {{row field="fechaNacimiento" label="Fecha Nacimiento" type="date" format="DD/MMM/YYYY"}}
        {{row field="horaNacimiento" label="Hora Nacimiento" type="date" format="h:mma"}}
        {{row field="genero" label="Género"}}
        {{row field="entidadNacimiento" label="Entidad Nacimiento"}}
