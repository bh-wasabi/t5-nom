{{#define id="persona"}}
  {{param defaultDisplay="_name"}}
  {{param defaultView="todos"}}
  {{param pageStart="1"}}
  {{param pagesJustified="true"}}
  {{param pagesTabsPosition="left"}}
  {{param pagesTabsWidth="10%"}}
  {{param pagesTabsMinPadding="false"}}
  {{param allowAdd="true"}}
  {{param allowEdit="true"}}
  {{param allowDirectOperations="true"}}
  {{param attachSection="adjuntos"}}
  {{param attachLanguage="es"}}
  {{param confirmUnSaved="true"}}
  {{param showChangeHistory="true"}}
  {{param pdfPageSize="letter"}}
  {{param startOnOpen="persona"}}
  {{param baseName="Datos Generales"}}
  {{param displayExpr="persona.nombre"}}
  {{param name="=_params._board||_parent.display||_name"}}
  {{param bannerTopTemplate="banner4"}}
  {{param addClass="no-padding-right"}}

  {{#menu id="menuFlujo" color="grey"}}
    {{item action="verSubProcesos" text="Ver Sub Procesos" userRole="verFlujoSubProcesos"}}
    {{item action="flujoResumen" text="Flujo Resumen" beginGroup="true" userRole="verFlujoResumen"}}
    {{item action="flujoGeneral" text="Flujo General" userRole="verFlujoGeneral"}}
    {{item action="flujoEjecucion" text="Flujo Ejecución" userRole="verFlujoEjecucion"}}
  {{/menu}}  
[.#if esSIC.]
  {{#action id="bim" type="bim360" label="BIM" title="=calc.concat('BIM', _items[0].persona.planta)" bim="=_items[0].persona.bim" color="grey" visibleMode="close" fromSource="persona" fromView="datosUbicacion"}}
    {{param ubicacion="=persona.ubicacion"}}
  {{/action}}
  {{#action id="planta" type="openPreview" label="Planta" title="Planta" color="grey" visibleMode="close" fromSource="persona" fromView="datosUbicacion"}}
    {{param ubicacion="=persona.ubicacion"}}
    {{link title="=calc.concat('Planta', _items[0].persona.planta)" href="=fn('urlPlanta', _items[0].persona.planta)"}}
  {{/action}}
  {{action id="menuFlujo" hide="true" type="menu" menu="menuFlujo" label="Flujo" color="grey" forceReadOnly="true"}}
[./if.]
[.#if esRH.]
  {{action id="menuFlujo" hide="true" type="menu" menu="menuFlujo" label="Flujo" color="grey" forceReadOnly="true"}}
[./if.]  
[.#if esMulti.]
  {{action id="flujo" type="bpmn" color="grey" label="Flujo" title="=calc.concatDash(_params._created.short, _params.persona.nombreCompleto)" visibleMode="close" bpmn="=calc.first(fn('bpmn', _params.base.tipoSolicitud, _params.base.subTipoSolicitud)).fileName" moment="=_params._updated.moment" momentColor="blue"}}
[./if.]  

  {{action id="flujoResumen"   type="bpmn" color="green"  label="Flujo Resumen"   title="_params._name||'Resumen'"               visibleMode="close" bpmn="=calc.first(fn('bpmn', 'resumen')).fileName" moment="=fn('resumenMomento', _params._updated.moment)" moments="=_params._moment"}}
  {{action id="flujoGeneral"   type="bpmn" color="orange" label="Flujo General"   title="_params._name||'Proceso General'"       visibleMode="close" bpmn="=calc.first(fn('bpmn', 'general')).fileName" moment="=_params._updated.moment" moments="=_params._moment" condition="=!calc.in(_params._updated.moment, ['entregado','finalizado'])"}}
  {{action id="flujoEjecucion" type="bpmn" color="blue"   label="Flujo Ejecución" title="_params._name||'Sub Proceso Ejecución'" visibleMode="close" bpmn="=calc.first(fn('bpmn', _params.base.tipoSolicitud, _params.base.subTipoSolicitud)).fileName" moment="=_params.control.subMoment||_params._updated.moment" moments="=_params._subMoment||_params._moment" condition="=calc.in(_params._updated.moment,['programado','programado2','entregado','finalizado'])||_params.control.subMoment"}}
  
  {{tileView id="resumen" height="100%" width="100%" itemMargin="15" baseItemHeight="130" baseItemWidth="130" direction="vertical" section="resumen" itemTemplate="resumen" orderBy="orden" controlFromMetadata="true"}}

  {{#section id="_temp" temp="true" beforeStart="true"}}
    {{field id="esPaciente" type="expr" value="=_parent.type=='paciente'"}}
    {{field id="esEmpleado" type="expr" value="=_parent.type=='_user'&&detalle.altaEmpleado.desde"}}
    {{field id="listaPrecios" type="expr" value="=fn('listaPrecios', detalle, contexto, persona)||calc.lookupConfigException('general', 'listaPrecios')||'price2'"}}
    {{field id="_listaPrecios" type="expr" value="=fn('_listaPrecios', detalle, contexto, persona)||calc.lookupConfigException('general', '_listaPrecios')||'N2'"}}
    {{field id="_gratuidad" type="expr" value="=listaPrecios=='price0'?'Gratuidad':'Pase a Caja'"}}
    {{field id="_embarazo" type="expr" value="=esPaciente&&fn('_semanasEmbarazo', fn('semanasEmbarazo', detalle.mujer))"}}
    {{field id="establishment" type="expr" value="=user.establishment"}}
    {{field id="_establishment" type="expr" value="=user._establishment"}}
    {{field id="logo" type="expr" value="=user.establishmentData.logo"}}
    {{field id="datosGenerales" type="expr" value="=user.establishmentData.datosGenerales"}}
  {{/section}}

