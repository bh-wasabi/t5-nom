        {{row field="religion" label="Religión"}}
        {{row field="formaContacto" label="Formas Contacto" }}
        {{row field="whatsApp" label="WhatsApp"}}
        {{row field="direccion" label="Domicilios"}}
        {{row field="ocupacion" label="Ocupación"}}
